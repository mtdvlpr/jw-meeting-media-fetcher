  "scripts": {
    "pre-build": "yarn prepare && ts-node -T pre-build.ts",
    "dev": "yarn pre-build && nuxi dev",
    "prepare": "nuxi prepare",
    "analyze": "cross-env NODE_OPTIONS=--max-old-space-size=4096 nuxi analyze",
    "build-dev": "yarn build --c electron-builder-dev.yml",
    "build": "yarn pre-build && cross-env NODE_OPTIONS=--max-old-space-size=4096 nuxi generate && electron-builder",
